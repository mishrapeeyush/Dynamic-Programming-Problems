class Solution{
  public:
    int longestPalinSubseq(string A) {
        //code here
        string B=A;
        reverse(A.begin(),A.end());
        
        int a=A.size();
        int b=B.size();
        
        int dp[a+1][b+1];
        
        for(int i=0;i<a+1;i++)
        {
            for(int j=0;j<b+1;j++)
            {
                if(i==0||j==0)
                {
                    dp[i][j]=0;
                }
            }
        }
        
        for(int i=1;i<a+1;i++)
        {
            for(int j=1;j<b+1;j++)
            {
                if(A[i-1]==B[j-1])
                {
                    dp[i][j]=dp[i-1][j-1]+1;
                }
                else
                dp[i][j]=max(dp[i-1][j], dp[i][j-1]);
            }
        }
        
        return dp[a][b];
    }
};
